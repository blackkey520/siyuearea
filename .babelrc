{
  "presets": [["es2015", { "modules": false }], "react", "stage-2"],
  "plugins": [
      "transform-runtime",
      "add-module-exports",
      "syntax-decorators",
      "transform-decorators-legacy",
      "transform-es2015-modules-commonjs",
      ["import", {
        "libraryName": "antd",
        "style": "css"
      }],
       ["import", {
         "libraryName": "antd-mobile",
         "style": true
       }]
    ]
}
